<!--Display IDs: VirtualSphere = 1, VirtualCube = 2, Sphere = 3, Cube = 4-->
<demoElement projectors ="2" 
             width ="700" 
             height="700" 
             display="VirtualSphere" 
             displayID="3"
             tracker="Mouse" 
             renderer ="OpenGL">
	<!-- Specify number of projectors-->
	<projectors>2</projectors>
  <width>700</width>
  
  <height>700</height>
	<!-- Specify Display: VirtualSphere, VirtualCube, Cube, Sphere, Flat-->
	<display>VirtualCube</display>

	<!-- Specify Tracker: Kinect, Fastrak-->
	<tracker>Fastrak</tracker>

	<!-- Specify Renderer: OpenGL, Unreal, Unity-->
	<renderer>OpenGL</renderer>

</demoElement>







